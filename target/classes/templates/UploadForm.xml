<?xml version="1.0" encoding="UTF-8"?>
<?language JavaScript?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbox" layoutX="10.0" layoutY="10.0" prefHeight="120.0" prefWidth="300.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="hotelicus.controllers.extended.Users.UploadUserForm">
    <style>
        -fx-padding: 10;
    </style>
    <children>
        <GridPane>
            <children>
                <Label text="Username:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <TextField fx:id="username" promptText="enter username" text="" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                <TextField fx:id="password" promptText="enter password" text="" GridPane.columnIndex="0" GridPane.rowIndex="3" />
                <Label text="First name:" GridPane.columnIndex="0" GridPane.rowIndex="4" />
                <TextField fx:id="firstName" promptText="enter first name" text="" GridPane.columnIndex="0" GridPane.rowIndex="5" />
                <Label text="Last name:" GridPane.columnIndex="0" GridPane.rowIndex="6" />
                <TextField fx:id="lastName" promptText="enter last name" text="" GridPane.columnIndex="0" GridPane.rowIndex="7" />
            </children>
            <columnConstraints>
                <ColumnConstraints maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
            </rowConstraints>
        </GridPane>
        <StackPane prefHeight="-1.0" prefWidth="-1.0">
            <children>
                <Button fx:id="saveButton" onAction="#uploadRouter" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Save" StackPane.alignment="CENTER_LEFT" />
            </children>
        </StackPane>
    </children>
</VBox>